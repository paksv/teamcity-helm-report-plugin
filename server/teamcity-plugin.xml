                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>HelmDiffSupport</name>
        <display-name>Helm Diff Support</display-name>
        <version>@Version@</version> <!-- will be replaced from script -->
